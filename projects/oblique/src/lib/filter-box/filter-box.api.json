{
  "$schema": "../../../schema.json",
  "api": [
    {
      "name": "ObFilterBoxComponent",
      "selector": "ob-filter-box",
      "exported": "obFilterBox",
      "since": "2.0.0",
      "inputs": [
        {
          "name": "pattern",
          "text": "The pattern property passed to the component. If defined, will serve as initial value.",
          "type": "string",
          "default": "undefined",
          "since": "2.0.0"
        },
        {
          "name": "placeholder",
          "text": "An i18n key that will be translated and used as placeholder.",
          "type": "string",
          "default": "i18n.common.filter.placeholder",
          "since": "2.0.0"
        },
        {
          "name": "size",
          "text": "The relative control sizing ",
          "type": "string",
          "default": "undefined",
          "values": [
            "sm",
            "lg"
          ],
          "since": "2.0.0"
        },
        {
          "name": "disabled",
          "text": "Defines if the nested input control should be disabled.",
          "type": "boolean",
          "default": "false",
          "since": "2.0.0"
        },
        {
          "name": "readonly",
          "text": "Defines if the nested input control should be read-only.",
          "type": "boolean",
          "default": "false",
          "since": "2.0.0"
        },
        {
          "name": "modelOptions",
          "text": "Options for the underlying <code>ngModel</code> directive.",
          "see": "https://angular.io/api/forms/NgModel#options",
          "type": "NgModelOptions",
          "default": "undefined",
          "since": "2.0.0"
        }
      ],
      "outputs": [
        {
          "name": "patternChange",
          "text": "Emits when the <code>pattern</code> is changed.",
          "type": "Observable<void>",
          "since": "2.0.0"
        },
        {
          "name": "patternClear",
          "text": "Emits when the <code>pattern</code> is cleared through the input's clear button.",
          "type": "Observable<void>",
          "since": "2.0.0"
        }
      ],
      "projection": [
        {
          "name": "<ng-template #prepend>",
          "text": "Content that will be prepend to the input",
          "since": "4.0.0"
        },
        {
          "name": "<ng-template #append>",
          "text": "Content that will be append to the input",
          "since": "4.0.0"
        }
      ]
    }
  ]
}
