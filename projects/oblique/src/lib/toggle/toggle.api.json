[
  {
    "name": "ToggleDirective",
    "selector": "[orToggle]",
    "exported": "orToggle",
    "since": "2.0.0",
    "inputs": [
      {
        "name": "orToggle",
        "text": "Defines the regular and active directions.",
        "type": "string",
        "default": "down-up",
        "values": [
          "down-up",
          "down-right",
          "down-left",
          "up-down",
          "up-right",
          "up-left",
          "right-left",
          "right-up",
          "right-down",
          "left-right",
          "left-up",
          "left-down"
        ],
        "since": "4.0.0"
      },
      {
        "name": "active",
        "text": "Indicates if the toggle should be activated, i.e. showing the 2nd direction.",
        "type": "boolean",
        "default": false,
        "since": "4.0.0"
      }
    ]
  }
]
